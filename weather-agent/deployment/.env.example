# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_CLOUD_STORAGE_BUCKET=your-bucket-name

# API Keys for LLMs
GOOGLE_API_KEY=your-google-api-key
OPENAI_API_KEY=your-openai-api-key  # Optional, only if using OpenAI models
ANTHROPIC_API_KEY=your-anthropic-api-key  # Optional, only if using Anthropic models

# Optional: Set to "True" to use Vertex AI for Gemini models instead of direct API
# GOOGLE_GENAI_USE_VERTEXAI=True